.stuHistory {
  margin-top: 20rpx;
  width: 100%;
  background: #FFFFFF;
}
.stuHistory .mess {
  text-align: center;
  color: #ccc;
}
.stuHistory .content .message {
  position: relative;
}
.stuHistory .content .message .dot {
  position: absolute;
  margin-left: 24rpx;
  top: 50.5rpx;
  /*margin-top:50.5rpx;*/
  width: 20rpx;
  height: 20rpx;
  /*line-height: 20rpx;*/
  /*text-align: center;*/
  display: flex;
  justify-content: center;
  align-items: center;
  background: rgba(254, 67, 69, 0.15);
  border-radius: 50%;
}
.stuHistory .content .message .dot text {
  display: inline-block;
  width: 10rpx;
  height: 10rpx;
  border-radius: 50%;
  background: #FE4345;
}
.stuHistory .content .message .describe {
  border-bottom: 1rpx solid #E9E9E9;
  width: 668rpx;
  /*border:1rpx solid red;*/
  margin-left: 64rpx;
  padding: 39.5rpx 0rpx;
}
.stuHistory .content .message .describe view {
  /*border:1rpx solid red;*/
  margin-top: 16rpx;
}
.stuHistory .content .message .describe view:first-of-type {
  margin-top: 0rpx;
  font-size: 30rpx;
  color: #FE4345;
  letter-spacing: 0;
  line-height: 40rpx;
  /*&:before{*/
  /*content:' ';*/
  /*display:inline-block;*/
  /*width:10rpx;*/
  /*height:10rpx;*/
  /*border-radius: 50%;*/
  /*background: #FE4345;*/
  /*}*/
}
.stuHistory .content .message .describe view:nth-of-type(2) text:first-of-type {
  font-size: 26rpx;
  color: #999999;
  letter-spacing: 0;
  line-height: 37rpx;
}
.stuHistory .content .message .describe view:nth-of-type(2) text:last-of-type {
  font-size: 26rpx;
  color: #222222;
  letter-spacing: 0;
  line-height: 37rpx;
}
.stuHistory .content .message .describe view:nth-of-type(3) text:first-of-type {
  display: inline-block;
  width: 182rpx;
  height: 37rpx;
  font-size: 26rpx;
  color: #999999;
  letter-spacing: 0;
  line-height: 37rpx;
}
.stuHistory .content .message .describe view:nth-of-type(3) text:last-of-type {
  font-size: 26rpx;
  color: #222222;
  letter-spacing: 0;
  line-height: 37rpx;
}
.stuHistory .content .message .describe view:nth-of-type(4) {
  display: flex;
}
.stuHistory .content .message .describe view:nth-of-type(4) text:first-of-type {
  display: inline-block;
  width: 182rpx;
  height: 37rpx;
  font-size: 26rpx;
  color: #999999;
  letter-spacing: 0;
  line-height: 37rpx;
}
.stuHistory .content .message .describe view:nth-of-type(4) view {
  display: inline-block;
  width: 440rpx;
  margin-right: 24rpx;
  font-size: 26rpx;
  color: #222222;
  letter-spacing: 0;
  line-height: 42rpx;
}
