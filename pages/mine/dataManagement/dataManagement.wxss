.dataManagement .container {
  /*padding-top: 128rpx;*/
  background: #F2F2F2;
  padding-bottom: 114rpx;
  /*padding-left:24rpx;*/
  /*padding-right:24rpx;*/
  display: flex;
  align-items: start;
  justify-content: space-around;
  flex-wrap: wrap;
}
.dataManagement .container .content {
  display: flex;
  flex-direction: column;
  justify-content: start;
  align-items: center;
  padding: 76rpx 0rpx;
  margin-top: 24rpx;
  width: 339rpx;
  height: 340rpx;
  background: #FFFFFF;
  box-shadow: 0 2rpx 20rpx 0 rgba(62, 23, 24, 0.05);
  border-radius: 6rpx;
}
.dataManagement .container .content text:first-of-type {
  display: inline-block;
  width: 110rpx;
  height: 110rpx;
  /*border:1rpx solid red;*/
  border-radius: 50%;
  margin-bottom: 36rpx;
  background-size: cover;
}
