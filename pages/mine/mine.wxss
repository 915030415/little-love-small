.mine .container .show .show-box {
  width: 702rpx;
  height: 120rpx;
  position: absolute;
  /*border:1rpx solid red;*/
  top: 40rpx;
}
.mine .container .show .show-box .picture {
  width: 120rpx;
  height: 120rpx;
  border: 4rpx solid rgba(255, 255, 255, 0.9);
  box-shadow: 0 2rpx 25rpx 0 rgba(0, 0, 0, 0.2);
  border-radius: 50%;
}
.mine .container .show .show-box .picture image {
  display: inline-block;
  width: 112rpx;
  height: 112rpx;
  border-radius: 50%;
}
.mine .container .show .show-box .message {
  position: absolute;
  top: 0rpx;
  left: 144rpx;
  display: inline-block;
  height: 120rpx;
  width: 555rpx;
