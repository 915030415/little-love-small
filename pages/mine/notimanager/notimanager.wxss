.notiManager {
  width: 100%;
  height: 1333rpx;
  background: #F2F2F2;
}
.notiManager .mess {
  margin-top: 300rpx;
  width: 100%;
  text-align: center;
  color: #ccc;
}
.notiManager .container {
  background: #F2F2F2;
}
.notiManager .container .box {
  background: #FFFFFF;
  height: 160rpx;
  /*border: 1rpx solid red;*/
  padding: 40rpx 0rpx 40rpx 24rpx;
  width: 750rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.notiManager .container .box view:first-of-type text:first-of-type {
  display: inline-block;
  width: 80rpx;
  height: 80rpx;
  /*background: #FFAD28;*/
  border-radius: 50%;
  background-size: cover;
}
.notiManager .container .box view:last-of-type {
  padding-right: 30rpx;
  margin-left: 20rpx;
  width: 670rpx;
  height: 160rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  border-bottom: 1rpx solid #ccc;
}
.notiManager .container .box view:last-of-type text:first-of-type {
  display: flex;
  justify-content: space-between;
}
.notiManager .container .box view:last-of-type text:first-of-type text:first-of-type {
  font-size: 30rpx;
  color: #000000;
  text-align: left;
  line-height: 36rpx;
}
.notiManager .container .box view:last-of-type text:first-of-type text:last-of-type {
  font-size: 24rpx;
  color: #B2B2B2;
  line-height: 28rpx;
}
.notiManager .container .box view:last-of-type text:last-of-type {
  margin-top: 16rpx;
  font-size: 26rpx;
  color: #999999;
  line-height: 28rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.notiManager .container .box:last-of-type view:last-of-type {
  border: none;
}
.notiManager .footer {
  width: 100%;
  height: 128rpx;
  background: #FFFFFF;
  box-shadow: 0 2rpx 20rpx 0 rgba(0, 0, 0, 0.05);
  border-radius: 2rpx;
  line-height: 128rpx;
  text-align: center;
}
.notiManager .footer text {
  display: inline-block;
  width: 640rpx;
  height: 80rpx;
  line-height: 80rpx;
  border: 1rpx solid #CECECE;
  box-shadow: 0 2rpx 20rpx 0 rgba(206, 206, 206, 0.2);
  border-radius: 44.5rpx;
}
