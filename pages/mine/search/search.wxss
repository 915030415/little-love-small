.search .container {
  padding-bottom: 64.5rpx;
  background-color: #ffffff;
}
.search .container .search {
  width: 750rpx;
  height: 138rpx;
  line-height: 138rpx;
  text-align: center;
  position: relative;
}
.search .container .search input {
  display: inline-block;
  width: 702rpx;
  height: 80rpx;
  background: #F5F5F5;
  border: 1rpx solid #E9E9E9;
  border-radius: 40rpx;
  font-size: 30rpx;
  letter-spacing: 0;
}
.search .container .search input::placeholder {
  color: #B2B2B2;
}
.search .container .search text {
  width: 22rpx;
  height: 22rpx;
  position: absolute;
  top: 58rpx;
  left: 54rpx;
  background-size: cover;
  /*border:1rpx solid red;*/
}
.search .container .box1 {
  /*border:1rpx solid red;*/
  width: 750rpx;
  display: flex;
  height: 92rpx;
  background-color: #E9E9E9;
  /*align-itmes:center;*/
}
.search .container .box1 text {
  height: 92rpx;
  line-height: 92rpx;
  flex: 1;
  text-align: center;
  font-size: 28rpx;
  color: #222222;
  letter-spacing: 0;
}
.search .container .box1 text.active {
  position: relative;
  font-size: 28rpx;
  color: #FE4345;
  letter-spacing: 0;
}
.search .container .box1 text.active:before {
  position: absolute;
  bottom: 0rpx;
  left: calc(50% - 38rpx);
  content: " ";
  display: inline-block;
  width: 76rpx;
  height: 4rpx;
  background: #F53907;
  border-radius: 8rpx;
}

.passList .mess {
  margin-top: 300rpx;
  width: 100%;
  text-align: center;
  color: #ccc;
}
.passList .box, .passList .visit {
  width: 750rpx;
  padding: 0rpx 24rpx;
  margin-top: 24rpx;
}
.passList .box .message, .passList .visit .message {
  width: 702rpx;
  height: 203.5rpx;
  border-bottom: 1rpx solid #E9E9E9;
  margin-bottom: 23.5rpx;
  position: relative;
}
.passList .box .message image, .passList .visit .message image {
  width: 180rpx;
  height: 180rpx;
  vertical-align: top;
}
.passList .box .message .means, .passList .visit .message .means {
  /*border: 1rpx solid red;*/
  width: 498rpx;
  height: 180rpx;
  position: absolute;
  top: 0rpx;
  right: 0rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}
.passList .box .message .means view, .passList .visit .message .means view {
  letter-spacing: 0;
  margin-top: 8rpx;
}
.passList .box .message .means view:nth-of-type(1), .passList .visit .message .means view:nth-of-type(1) {
  font-size: 30rpx;
  color: #000000;
  line-height: 44rpx;
}
.passList .box .message .means view:nth-of-type(2), .passList .visit .message .means view:nth-of-type(2) {
  font-size: 26rpx;
  color: #666666;
}
.passList .box .message .means view:nth-of-type(3), .passList .visit .message .means view:nth-of-type(3) {
  font-size: 26rpx;
  color: #222222;
  wdith: 494rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  line-height: 37rpx;
}
.passList .box .message .means view:nth-of-type(4), .passList .visit .message .means view:nth-of-type(4) {
  font-size: 26rpx;
  color: #F9AA21;
}
.passList .box .message .means text.status, .passList .box .message .means text.time, .passList .visit .message .means text.status, .passList .visit .message .means text.time {
  position: absolute;
  right: 24rpx;
  top: 10rpx;
  font-size: 26rpx;
  color: #24B684;
  letter-spacing: 0;
  line-height: 44rpx;
}
.passList .visit .message .means {
  justify-content: center;
}
.passList .visit .message .means text.time {
  top: 25rpx;
}
