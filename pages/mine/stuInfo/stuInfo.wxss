.stuInfo .container {
  padding-bottom: 128rpx;
  background-color: #E5E5E5;
  width: 750rpx;
  min-height: 1334rpx;
}
.stuInfo .container .infoSlide {
  border-top: 12rpx solid #E5E5E5;
}
.stuInfo .container .infoSlide .box {
  background: #ffffff;
  width: 100%;
  display: flex;
  justify-content: space-around;
  align-items: center;
  /*.slide.active:hover {*/
  /*text {*/
  /*color: #FE4345;*/
  /*}*/
  /*}*/
  /*.slide.active {*/
  /*text {*/
  /*color: #FE4345;*/
  /*}*/
  /*}*/
}
.stuInfo .container .infoSlide .box view.slide {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  padding: 25.7rpx 0rpx 10rpx;
}
.stuInfo .container .infoSlide .box view.slide text:first-of-type {
  display: inline-block;
  width: 29.9rpx;
  height: 32.8rpx;
  /*border: 1rpx solid red;*/
  background-size: cover;
}
.stuInfo .container .infoSlide .box view.slide text:last-of-type {
  font-size: 26rpx;
  color: #222222;
  letter-spacing: 0;
  margin-top: 12rpx;
}
.stuInfo .container .infoSlide .box view.slide.active {
  position: relative;
}
.stuInfo .container .infoSlide .box view.slide.active text:first-of-type {
  /*border: 1rpx solid red;*/
}
.stuInfo .container .infoSlide .box view.slide.active text {
  color: #FE4345;
}
.stuInfo .container .infoSlide .box view.slide.active:after {
  content: " ";
  position: absolute;
  /*width:28rpx;*/
  /*height:28rpx;*/
  /*border-bottom:14rpx solid transparent;*/
  bottom: 0rpx;
  border-top: 14rpx solid transparent;
  border-right: 14rpx solid transparent;
  border-left: 14rpx solid transparent;
  border-bottom: 14rpx solid #D8D8D8;
  /*background: #D8D8D8;*/
  /*transform:translateY(50%)*/
}
.stuInfo .container .stuHistory, .stuInfo .container .stuFamily {
  margin-top: 20rpx;
}

