.backMoney {
  padding-bottom: 20rpx;
}
.backMoney .photo {
  position: fixed;
  z-index: 1000;
  top: 0rpx;
  left: 0rpx;
  background-color: #000;
  /*padding-top:500rpx;*/
  width: 750rpx;
  height: 1334rpx;
}
.backMoney .photo image {
  width: 750rpx;
  height: 400rpx;
}
.backMoney .fadeIn {
  animation: animation_inp 0.25s;
  animation-fill-mode: forwards;
}
.backMoney .fadeOut {
  animation: animation_outp 0.25s;
}
@keyframes animation_inp {
  0% {
    padding-top: 0rpx;
  }
  100% {
    padding-top: 200rpx;
  }
}
@keyframes animation_outp {
  0% {
    padding-top: 200rpx;
  }
  100% {
    padding-top: 0rpx;
  }
}
.backMoney .mess {
  margin-top: 20rpx;
  width: 100%;
  text-align: center;
  color: #ccc;
}
.backMoney .box {
  width: 670rpx;
  background: #FFFFFF;
  border-radius: 8rpx;
  margin: 20rpx 20rpx 0rpx 60rpx;
  padding: 24rpx 24rpx 28rpx;
  height: 438rpx;
}
.backMoney .box view:last-of-type image {
  display: inline-block;
  width: 622rpx;
  height: 280rpx;
}
.backMoney .box view:nth-of-type(2) {
  margin: 16rpx 0rpx;
}
.backMoney .box view:nth-of-type(1) text:first-of-type, .backMoney .box view:nth-of-type(2) text:first-of-type {
  font-size: 26rpx;
  color: #999999;
  letter-spacing: 0;
}
.backMoney .box view:nth-of-type(1) text:last-of-type, .backMoney .box view:nth-of-type(2) text:last-of-type {
  font-size: 26rpx;
  color: #222222;
  letter-spacing: 0;
}

